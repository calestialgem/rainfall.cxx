{
  "version": "0.2.0",
  "configurations": [
    /// Run after rebuilding for Windows.
    {
      "name": "Windows Launch",
      "type": "cppvsdbg",
      "request": "launch",
      "preLaunchTask": "${defaultBuildTask}",
      "program": "${workspaceFolder}/build/rainfall.exe",
      "cwd": "${workspaceFolder}/res",
      "args": ["thrice/demo", "orderofoperations"],
      "console": "integratedTerminal"
    }
  ]
}
